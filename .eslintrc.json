{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "react-app",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": "off",
    "indent": "off"
  }
}
